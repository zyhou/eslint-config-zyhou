{
    "name": "eslint-config-zyhou",
    "version": "0.0.6",
    "description": "Personal eslint config",
    "main": "base.js",
    "files": [
        "base.js",
        "cypress.js",
        "jest.js",
        "jsdoc.js",
        "react.js"
    ],
    "repository": "git@github.com:zyhou/eslint-config-zyhou.git",
    "bugs": {
        "url": "https://github.com/zyhou/eslint-config-zyhou/issues"
    },
    "homepage": "https://github.com/zyhou/eslint-config-zyhou",
    "keywords": [
        "eslint",
        "eslint-config"
    ],
    "author": "Maxime Richard",
    "license": "MIT",
    "scripts": {
        "release": "standard-version",
        "lint": "eslint .",
        "format": "prettier --write --check \"**/*.+(js|json|md)\""
    },
    "peerDependencies": {
        "eslint": ">=7.9.0",
        "prettier": ">=2.0.2"
    },
    "devDependencies": {
        "@commitlint/cli": "11.0.0",
        "@commitlint/config-conventional": "11.0.0",
        "eslint": "7.9.0",
        "husky": "4.3.0",
        "lint-staged": "10.4.0",
        "prettier": "2.1.2",
        "standard-version": "9.0.0"
    },
    "dependencies": {
        "babel-eslint": "^10.1.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-cypress": "^2.10.3",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jest": "^24.0.2",
        "eslint-plugin-jest-dom": "^3.2.3",
        "eslint-plugin-jsdoc": "^30.5.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^4.0.0"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": "eslint",
        "*.{js,json,md}": "prettier --write"
    }
}
