{
    "name": "eslint-config-zyhou",
    "version": "0.0.8",
    "description": "Personal eslint config",
    "main": "base.js",
    "files": [
        "base.js",
        "react.js",
        "typescript.js",
        "tests.js",
        "presets"
    ],
    "repository": "git@github.com:zyhou/eslint-config-zyhou.git",
    "bugs": {
        "url": "https://github.com/zyhou/eslint-config-zyhou/issues"
    },
    "homepage": "https://github.com/zyhou/eslint-config-zyhou",
    "keywords": [
        "eslint",
        "eslint-config"
    ],
    "author": "Maxime Richard",
    "license": "MIT",
    "scripts": {
        "release": "standard-version",
        "lint": "eslint .",
        "format": "prettier --write --check \"**/*.+(js|json|md)\""
    },
    "peerDependencies": {
        "eslint": ">=7.26.0",
        "prettier": ">=2.3.0"
    },
    "devDependencies": {
        "@commitlint/cli": "12.1.1",
        "@commitlint/config-conventional": "12.1.1",
        "eslint": "7.26.0",
        "husky": "6.0.0",
        "lint-staged": "11.0.0",
        "prettier": "2.3.0",
        "standard-version": "9.3.0",
        "typescript": "4.2.4"
    },
    "dependencies": {
        "@babel/eslint-parser": "^7.13.14",
        "@babel/eslint-plugin": "^7.13.16",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-cypress": "^2.11.3",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-jest-dom": "^3.9.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-testing-library": "^4.2.1",
        "eslint-plugin-unicorn": "^32.0.1"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "lint-staged": {
        "*.js": "eslint",
        "*.{js,json,md}": "prettier --write"
    }
}
